<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :first_name %>
    <%= form.text_field :first_name, id: :profile_first_name %>
  </div>

  <div class="field">
    <%= form.label :last_name %>
    <%= form.text_field :last_name, id: :profile_last_name %>
  </div>

  <div class="field">
    <%= form.label :date_of_birth %>
    <%= form.date_select :date_of_birth, id: :profile_date_of_birth %>
  </div>

  <div class="field">
    <%= form.label :display_picture_data %>
    <%= form.text_area :display_picture_data, id: :profile_display_picture_data %>
  </div>

  <div class="field">
    <%= form.label :cover_picture_data %>
    <%= form.text_area :cover_picture_data, id: :profile_cover_picture_data %>
  </div>

  <div class="field">
    <%= form.label :about_me %>
    <%= form.text_area :about_me, id: :profile_about_me %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id, id: :profile_user_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
